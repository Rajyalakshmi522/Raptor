        "# linear search\n",
        "def linearsearch(list,key):\n",
        "  for i in range(0,len(list)):\n",
        "    if key==list[i]:\n",
        "      print(\"key is found\")\n",
        "      break\n",
        "  else:\n",
        "    print(\"key is not found\")\n",
        "list=[90,123,324,21,56]\n",
        "print(list)\n",
        "key=int(input(\"enter key value\"))\n",
        "linearsearch(list,key)      \n"
      ],
      "execution_count": 0,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "[90, 123, 324, 21, 56]\n",
            "enter key value21\n",
            "key is found\n"
          ],
          "name": "stdout"
        }
      ]
