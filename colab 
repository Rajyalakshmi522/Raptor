        "# strong number\n",
        "n=int(input(\"enter n value\"))\n",
        "temp=n\n",
        "sum=0\n",
        "while n!=0:\n",
        "  f=1\n",
        "  d=n%10\n",
        "  for i in range(d,0,-1):\n",
        "    f=f*i\n",
        "  sum=sum+f\n",
        "  n=n//10\n",
        "if sum==temp:\n",
        "  print(\"strong\")\n",
        "else:\n",
        "  print(\"not stromg\")  "
      ],
      "execution_count": 0,
      "outputs": [
