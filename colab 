        "# factorial using recursion\n",
        "def fact(n):\n",
        "  if n==0:\n",
        "    return 1\n",
        "  else:\n",
        "      return n*fact(n-1)\n",
        "n=int(input(\"enter n value\"))            \n",
        "result=fact(n)\n",
        "print(result)\n"
      ],
      "execution_count": 0,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
